{
  "name": "@jurjanpaul/codemirror6-parinfer",
  "version": "0.0.2",
  "description": "Parinfer integration for CodeMirror 6",
  "license": "MIT",
  "type": "module",
  "keywords": [
    "parinfer",
    "codemirror",
    "codemirror6"
  ],
  "author": "Jurjan-Paul Medema",
  "publishConfig": {
   "access": "public"
  },
  "files": [
    "dist/*"
  ],
  "exports": {
   "import": "./dist/cm6-parinfer.js"
  },
  "module": "dist/cm6-parinfer.js",
  "dependencies": {
    "@codemirror/commands": "6.7.1",
    "@codemirror/lint": "6.8.4",
    "@codemirror/merge": "6.7.4",
    "@codemirror/state": "6.5.0",
    "@codemirror/view": "6.36.1",
    "@nextjournal/lang-clojure": "1.0.0",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "codemirror": "6.0.1",
    "parinfer": "3.13.1"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "eslint": "^9.17.0",
    "nodemon": "^3.1.9",
    "prettier": "^3.4.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  },
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "lint": "eslint ts/**/*.{js,ts}",
    "format": "prettier --write \"ts/**/*.ts\""
  }
}
